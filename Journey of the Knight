//https://www.codechef.com/JULY221C/problems/KNIGHT2/

#include <iostream>
using namespace std;
#define ll long long int
int main() {
	// your code goes here
	ll t;
	cin>>t;
	while(t--){
	    ll x1,y1,x2,y2;
	    cin>>x1>>y1>>x2>>y2;
	    ll x=abs(x1-x2);
	    ll y=abs(y1-y2);
	    if(x%2!=0 && y%2!=0 || x%2==0&&y%2==0) cout<<"YES"<<endl;
	    else cout<<"NO"<<endl;
	}
	return 0;
}
